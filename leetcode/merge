def merge(nums1, m, nums2, n):
    """
        :type nums1: List[int]
        :type m: int
        :type nums2: List[int]
        :type n: int
        :rtype: None Do not return anything, modify nums1 in-place instead.
    """
    if m == 0:
        nums1 = []
    else:
        nums1 = nums1[:m]
    for num in nums2:
        nums1.append(num)
        nums1 = sorted(nums1)
    return nums1

print(merge([0], 0, [1], 1))