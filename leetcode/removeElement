def removeElement(nums, val):
        """
        :type nums: List[int]
        :type val: int
        :rtype: int
        """

        k = 0  # 指向下一個要放的位置
        
        for i in range(len(nums)):
            if nums[i] != val:
                nums[k] = nums[i]
                k += 1
        
        return k
                
print(removeElement([3,2,2,3], 3))